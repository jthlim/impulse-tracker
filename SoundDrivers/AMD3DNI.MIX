        PI2FD           MM0, MM0        ; MM0 = FS2|FS1
        PI2FD           MM1, MM1        ; MM1 = FS6|FS5
        PI2FD           MM2, MM2        ; MM2 = FS4|FS3
        PI2FD           MM3, MM3        ; MM3 = FS8|FS7

        MovQ            MM4, MM0
